<?xml version="1.0" encoding="UTF-8"?>
<properties:PropertyDescriptorGroup xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:properties="http://goedegep.properties/model"
    name="Vacations" description="Standard settings for the Vacations application.">
  <propertyDescriptors xsi:type="properties:FilePropertyDescriptor" type="FILE" name="vacationsFileName"
      description="Name of the file with information about all vacations." displayName="Vacations file name">
    <fileExtensions>xmi</fileExtensions>
  </propertyDescriptors>
  <propertyDescriptors xsi:type="properties:FilePropertyDescriptor" type="FILE" name="vacationChecklistFileName"
      description="Name of the file with the vacation checklist information." displayName="Vacation checklist file name">
    <fileExtensions>xmi</fileExtensions>
  </propertyDescriptors>
  <propertyDescriptors type="DIRECTORY" name="vacationsFolderName" description="Name of the folder with information about vacations."
      displayName="Vacations folder name"/>
  <propertyDescriptors type="DIRECTORY" name="vacationPicturesFolderName" description="Name of the folder with vacation pictures."
      displayName="Vacation pictures folder name"/>
  <propertyDescriptors name="knownFiles" description="List of known files in the vacations folder."
      displayName="Known files"/>
  <propertyDescriptors name="ignoreVacationPictureFolders" description="List of folder names which are to be ignored in a vacation pictures folder."
      displayName="Picture folders to ignore"/>
</properties:PropertyDescriptorGroup>
