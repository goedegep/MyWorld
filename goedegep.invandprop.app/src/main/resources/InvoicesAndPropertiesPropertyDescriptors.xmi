<?xml version="1.0" encoding="UTF-8"?>
<properties:PropertyDescriptorGroup xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:properties="http://goedegep.properties/model"
    name="InvoicesAndProperties" packageName="goedegep.invandprop.app"
    registryClassName="InvoicesAndPropertiesRegistry">
  <propertyDescriptors xsi:type="properties:FilePropertyDescriptor" type="FILE" name="configurationFile"
      description="Name of the file with configuration information." displayName="Configuration filename"
      initialValue="InvoicesAndPropertiesConfiguration.xmi" installInitialValue="true">
    <fileExtensions>xmi</fileExtensions>
  </propertyDescriptors>
  <propertyDescriptors xsi:type="properties:FilePropertyDescriptor" type="FILE" name="customPropertiesFile"
      description="Name of the file with user settings." displayName="User settings filename"
      initialValue="InvoicesAndPropertiesSettings.xmi" installInitialValue="true">
    <fileExtensions>xmi</fileExtensions>
  </propertyDescriptors>
  <propertyDescriptors xsi:type="properties:FilePropertyDescriptor" type="FILE" name="invoicesAndPropertiesFile"
      description="Name of the file with all invoices and properties" displayName="Invoices and properties file"
      initialValue="D:\Database\MyWorld\InvoicesAndProperties.xmi" userSettable="true" installInitialValue="true"/>
  <propertyDescriptors type="DIRECTORY" name="propertyRelatedFilesFolder" description="Folder with property related files (documents and pictures)"
      displayName="Property related files folder" initialValue="D:\Database\Handleidingen"
      userSettable="true" installInitialValue="true"/>
  <propertyDescriptors name="shortProductInfo" description="Short product information."
      displayName="Short product information" initialValue="A database like application to store information about Invoices and Properties."
      installInitialValue="true"/>
  <propertyDescriptors name="version" description="Software version information." displayName="Version"
      initialValue="1.0" installInitialValue="true"/>
  <propertyDescriptors name="copyrightMessage" description="Copyright information."
      displayName="Copyright Text" initialValue="Copyright (c) 2001-2021" installInitialValue="true"/>
  <propertyDescriptors name="author" description="Author of this program." displayName="Author"
      initialValue="Peter Goedegebure" installInitialValue="true"/>
</properties:PropertyDescriptorGroup>
