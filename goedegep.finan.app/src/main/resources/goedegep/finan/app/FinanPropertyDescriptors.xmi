<?xml version="1.0" encoding="UTF-8"?>
<properties:PropertyDescriptorGroup xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:properties="http://goedegep.properties/model"
    name="Finan">
  <propertyDescriptors xsi:type="properties:FilePropertyDescriptor" type="FILE" name="configurationFile"
      description="Name of the file with configuration information." displayName="Configuration filename">
    <fileExtensions>xmi</fileExtensions>
  </propertyDescriptors>
  <propertyDescriptors type="DIRECTORY" name="dataDirectory"
      description="Folder with all financial information files."
      displayName="Finan data folder"/>
  <propertyDescriptors xsi:type="properties:FilePropertyDescriptor" type="FILE" name="jobAppointmentFile"
      description="Name of the file with the job appointments." displayName="Job appointment file name">
    <fileExtensions>job</fileExtensions>
  </propertyDescriptors>
  <propertyDescriptors xsi:type="properties:FilePropertyDescriptor" type="FILE" name="mortgagesFileName"
      description="Name of the file containing the mortgages information" displayName="Mortgages filename"/>
    <fileExtensions>xml</fileExtensions>
  </propertyDescriptors>
  <propertyDescriptors xsi:type="properties:FilePropertyDescriptor" type="FILE" name="investmentInsurancesFileName"
      description="Name of the file with information on all investment insurances." displayName="Investment insurances filename">
    <fileExtensions>invins</fileExtensions>
  </propertyDescriptors>
</properties:PropertyDescriptorGroup>
