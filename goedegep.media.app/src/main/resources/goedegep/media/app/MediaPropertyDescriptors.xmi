<?xml version="1.0" encoding="UTF-8"?>
<properties:PropertyDescriptorGroup xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:properties="http://goedegep.properties/model"
    name="Media">
  <propertyDescriptors xsi:type="properties:FilePropertyDescriptor" type="FILE" name="mediaDbFile"
      description="Name of the file with media information." displayName="Media database filename">
    <fileExtensions>xmi</fileExtensions>
  </propertyDescriptors>
  <propertyDescriptors type="DIRECTORY" name="musicDataDirectory" description="Directory where pictures of albums and artists are located."
      displayName="Music data directory"/>
  <propertyDescriptors type="DIRECTORY" name="musicDirectory" description="Folder with all music."
      displayName="Music folder"/>
  <propertyDescriptors type="DIRECTORY" name="albumImportDirectory" description="Default location for selecting a directory from which album import information is to be derived."
      displayName="Album Import Directory"/>
  <propertyDescriptors type="DIRECTORY" name="duneMusicFolderPath" description="Folder with all music on the Dune media player."
      displayName="Dune Music Folder Path"/>
  <propertyDescriptors type="DIRECTORY" name="dunePlaylistsFolderPath" description="Folder with all playlists on the Dune media player."
      displayName="Dune Playlist Folder Path"/>
  <propertyDescriptors type="DIRECTORY" name="photosFolder" description="Top level folder of all photos."
      displayName="Photo folder"/>
  <propertyDescriptors name="ignoreFolderNames" description="List of folder names to be ignored."
      displayName="Ignore folders"/>
  <propertyDescriptors xsi:type="properties:FilePropertyDescriptor" type="FILE" name="mediaPlayerClassicExecutable"
      description="Full path to the MPC-BE media player." displayName="Media Player Classic (MPC-BE) file">
    <fileExtensions>xmi</fileExtensions>
  </propertyDescriptors>
</properties:PropertyDescriptorGroup>
