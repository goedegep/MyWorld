<?xml version="1.0" encoding="UTF-8"?>
<properties:PropertyDescriptorGroup
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:properties="http://goedegep.properties/model"
    name="Rolodex"
    packageName="goedegep.rolodex.app"
    registryClassName="RolodexRegistry">
  <propertyDescriptors
      xsi:type="properties:FilePropertyDescriptor"
      type="FILE"
      name="configurationFile"
      description="Name of the file with configuration information."
      displayName="Configuration filename"
      initialValue="RolodexConfiguration.xmi"
      installInitialValue="true">
    <fileExtensions>xmi</fileExtensions>
  </propertyDescriptors>
  <propertyDescriptors
      xsi:type="properties:FilePropertyDescriptor"
      type="FILE"
      name="customPropertiesFile"
      description="Name of the file with user settings."
      displayName="User settings filename"
      initialValue="RolodexSettings.xmi"
      installInitialValue="true">
    <fileExtensions>xmi</fileExtensions>
  </propertyDescriptors>
  <propertyDescriptors
      xsi:type="properties:FilePropertyDescriptor"
      type="FILE"
      name="rolodexFile"
      description="Name of the file with the rolodex data."
      displayName="Rolodex filename"
      initialValue="D:\Database\MyWorld\Rolodex.xmi"
      userSettable="true"
      installInitialValue="true">
    <fileExtensions>xmi</fileExtensions>
  </propertyDescriptors>
</properties:PropertyDescriptorGroup>
